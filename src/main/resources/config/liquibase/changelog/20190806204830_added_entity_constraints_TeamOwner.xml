<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity TeamOwner.
    -->
    <changeSet id="20190806204830-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="team_owner"
                                 constraintName="fk_team_owner_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="team_owner"
                                 constraintName="fk_team_owner_owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="owner"/>

        <addForeignKeyConstraint baseColumnNames="effective_week_id"
                                 baseTableName="team_owner"
                                 constraintName="fk_team_owner_effective_week_id"
                                 referencedColumnNames="id"
                                 referencedTableName="week"/>

    </changeSet>
</databaseChangeLog>
